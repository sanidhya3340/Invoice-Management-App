<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Main_backend</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
<filter>
<filter-name>CorsFilter</filter-name>
<filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
<init-param>
<param-name>cors.allowed.origins</param-name>
<param-value>http://localhost:3000</param-value>
</init-param>
</filter>
<filter-mapping>
<filter-name>CorsFilter</filter-name>
<url-pattern>/*</url-pattern>
</filter-mapping>
<servlet>
<servlet-name>Fetch</servlet-name>
<servlet-class>com.servelet.Fetch</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Fetch</servlet-name>
<url-pattern>/Fetch.do</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Fetchten</servlet-name>
<servlet-class>com.servelet.Fetchten</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Fetchten</servlet-name>
<url-pattern>/Fetchten.do</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Add</servlet-name>
<servlet-class>com.servelet.Add</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Add</servlet-name>
<url-pattern>/Add.do</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Adding</servlet-name>
<servlet-class>com.servelet.Adding</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Adding</servlet-name>
<url-pattern>/Adding.do</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Updating</servlet-name>
<servlet-class>com.servelet.Updating</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Updating</servlet-name>
<url-pattern>/Updating.do</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Deleting</servlet-name>
<servlet-class>com.servelet.Deleting</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Deleting</servlet-name>
<url-pattern>/Deleting.do</url-pattern>
</servlet-mapping>
</web-app>